{
   "version": 2,
   "builds": [
     {
       "src": "app.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "app.js"
     },
     {
       "src": "/(.*)",
       "dest": "assests/$1"
     }
   ],
   "env": {
     "NODE_ENV": "production"
   }
 }